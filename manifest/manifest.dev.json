{
  "short_name": "SMS Sender",
  "name": "SMS Sender",
  "version": "1.0.1",
  "manifest_version": 2,
  "browser_action": {},
  "background": {
    "scripts": ["assets/background.js"],
    "persistent": true
  },
  "web_accessible_resources": ["/*"],
  "permissions": [
    "http://localhost:3000/*",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' http://localhost:3000 'unsafe-eval'; object-src 'self'"
}
